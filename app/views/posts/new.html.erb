<h1>New Post</h1>

<%= form_for @post do |f| %>
  <%= f.label :body %>
  <%= f.text_field :body %>
  <%= f.label :images %>
  <%= f.file_field :images, multiple: true %>
  <%= f.hidden_field :user_id, value: params[:user_id].to_i %>
  <%= f.submit %>
<% end %>
<p>Find me in app/views/posts/new.html.erb</p>
